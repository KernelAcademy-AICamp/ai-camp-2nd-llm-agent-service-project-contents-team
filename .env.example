# Hugging Face API Key (이미지 생성용)
# https://huggingface.co/settings/tokens 에서 생성
HUGGINGFACE_API_KEY=your_huggingface_api_key_here

# Anthropic API Key (나노바나나 - Claude로 프롬프트 최적화)
# https://console.anthropic.com/settings/keys 에서 생성
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Google API Key (제미나이 - Gemini로 프롬프트 최적화)
# https://makersuite.google.com/app/apikey 에서 생성
GOOGLE_API_KEY=your_google_api_key_here

# 서버 포트
PORT=5001

# 개발 환경 설정
NODE_ENV=development
